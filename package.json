{
  "name": "cvhere",
  "version": "1.0.0",
  "description": "CVHere - Privacy-first CV building and matching platform",
  "main": "backend/src/app.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon backend/src/app.js",
    "dev:frontend": "cd frontend && vite",
    "build": "npm run build:frontend",
    "build:frontend": "cd frontend && vite build",
    "test": "jest",
    "test:backend": "jest backend/",
    "test:frontend": "cd frontend && vitest run",
    "start": "node backend/src/app.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "express-session": "^1.17.3",
    "connect-pg-simple": "^9.0.1",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "vite": "^5.0.10",
    "@vitejs/plugin-react": "^4.2.1",
    "axios": "^1.6.2",
    "joi": "^17.11.0",
    "prisma": "^5.7.1",
    "@prisma/client": "^5.7.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "concurrently": "^8.2.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "vitest": "^1.1.0",
    "jsdom": "^23.0.1"
  },
  "keywords": ["cvhere", "cv", "resume", "matching", "privacy", "nodejs", "react"],
  "author": "CVHere Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
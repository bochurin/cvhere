{
  "name": "cvhere",
  "version": "1.0.0",
  "description": "CVHere - Privacy-first CV building and matching platform",
  "main": "backend/src/app.ts",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon --exec ts-node backend/src/app.ts",
    "dev:frontend": "cd frontend && vite",
    "build": "npm run build:frontend",
    "build:frontend": "cd frontend && vite build",
    "test": "jest",
    "test:backend": "jest backend/",
    "test:frontend": "cd frontend && vitest run",
    "start": "node backend/src/app.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.879.0",
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^8.0.3",
    "@prisma/client": "^6.15.0",
    "@sendgrid/mail": "^8.1.5",
    "@types/node": "^20.10.4",
    "@vitejs/plugin-react": "^4.2.1",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "connect-pg-simple": "^9.0.1",
    "dotenv": "^16.3.1",
    "fastify": "^4.24.3",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.10"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "jsdom": "^23.0.1",
    "nodemon": "^3.0.2",
    "prisma": "^6.15.0",
    "supertest": "^6.3.3",
    "vitest": "^1.1.0"
  },
  "keywords": [
    "cvhere",
    "cv",
    "resume",
    "matching",
    "privacy",
    "nodejs",
    "react"
  ],
  "author": "CVHere Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}

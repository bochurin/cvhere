{
  "name": "cvhere",
  "version": "1.0.0",
  "description": "CV building and searching application",
  "scripts": {
    "dev:backend": "ts-node backend/src/server.ts",
    "build:backend": "tsc -p backend/tsconfig.json",
    "start:backend": "node backend/dist/server.js",
    "dev:frontend": "cd frontend && npm run dev",
    "build:frontend": "cd frontend && npm run build",
    "install:frontend": "cd frontend && npm install"
  },
  "dependencies": {
    "fastify": "^4.24.3",
    "@fastify/cors": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}